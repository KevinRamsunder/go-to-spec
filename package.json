{
  "name": "go-to-test",
  "displayName": "Go to Test",
  "description": "Switch between code and test file",
  "version": "0.1.2",
  "publisher": "k-r",
  "homepage": "https://github.com/kevinramsunder/go-to-spec",
  "repository": {
    "type": "git",
    "url": "https://github.com/kevinramsunder/go-to-spec.git"
  },
  "bugs": {
    "url": "https://github.com/kevinramsunder/go-to-spec/issues"
  },
  "engines": {
    "vscode": "^1.22.0"
  },
  "categories": [
    "Other"
  ],
  "keywords": [
    "test",
    "jest"
  ],
  "activationEvents": [
    "onCommand:extension.goToSpec"
  ],
  "main": "./extension",
  "contributes": {
    "commands": [
      {
        "command": "extension.goToSpec",
        "title": "Go to Spec"
      }
    ],
    "keybindings": [
      {
        "command": "extension.goToSpec",
        "key": "ctrl+shift+t",
        "mac": "cmd+shift+t",
        "when": "editorTextFocus"
      }
    ]
  },
  "scripts": {
    "postinstall": "node ./node_modules/vscode/bin/install",
    "test": "node ./node_modules/vscode/bin/test"
  },
  "devDependencies": {
    "@types/mocha": "^2.2.42",
    "@types/node": "^8.10.0",
    "standard": "^11.0.1",
    "vscode": "^1.1.6"
  }
}
